{
	"name": "frontend",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "npm run lint:eslint . && npm run lint:prettier .",
		"lint:nowarns": "npm run lint:eslint:nowarns . && npm run lint:prettier .",
		"lint:fix": "npm run lint:prettier:fix . && npm run lint:eslint:fix .",
		"lint:eslint": "eslint ",
		"lint:eslint:nowarns": "eslint --quiet",
		"lint:eslint:fix": "npm run lint:eslint -- --fix",
		"lint:prettier": "prettier --check --plugin-search-dir=.",
		"lint:prettier:fix": "npm run lint:prettier -- -w",
		"lint:svelte-check": "svelte-check --tsconfig ./tsconfig.json --use-new--transformation --output machine",
		"lint:svelte-check:ignore-gen": "svelte-check --use-new--transformation --output machine --ignore \".svelte-kit,build,src/gen\"",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "cd .. && husky install frontend/.husky"
	},
	"devDependencies": {
		"@aws-amplify/api": "^5.4.4",
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.25.0",
		"@types/cookie": "^0.5.2",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"autoprefixer": "^10.4.15",
		"aws-amplify": "^5.3.10",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.33.1",
		"flowbite-svelte": "^0.44.15",
		"flowbite-svelte-blocks": "^0.5.1",
		"flowbite-svelte-icons": "^0.4.4",
		"flowbite-typography": "^1.0.3",
		"husky": "^8.0.3",
		"i": "^0.3.7",
		"pnpm": "^8.7.5",
		"postcss": "^8.4.29",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"svelte-typewriter": "^3.1.6",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "^0.15.0",
		"@auth/sveltekit": "^0.3.7",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"amazon-cognito-identity-js": "^6.3.6"
	}
}
